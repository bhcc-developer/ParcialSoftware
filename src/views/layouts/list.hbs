 <div class="container p-4">

    <div id="container"></div>

    <div class='buttons mt-3'>
    </div>
    


        <h1 class="text-center mt-5">Cantidad de pruebas para detectar 1 caso</h1>
        <div class="row">
            
            

            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col">Pa√≠s</th>
                        <th scope="col">Pruebas</th>
                        <th scope="col">Test</th>
                        <th scope="col">Total</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each top10}}
                    <tr>
                    
                        <td>
                            <img src="/public/banderas/{{pais}}.png" alt="100">    
                            <span class="ml-4"> {{pais}}  </span>
                        </td>
                        <td>{{pruebas}}</td>
                        <td>{{test}}</td>
                        <td>{{total}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>


